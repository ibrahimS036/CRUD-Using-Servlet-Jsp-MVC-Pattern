<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CrudMVC</display-name>
  <welcome-file-list>
    <welcome-file>Form.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>fast</servlet-name>
  <servlet-class>MvcPackage.CreateServlet</servlet-class>
  </servlet>
  
   <servlet-mapping>
  <servlet-name>fast</servlet-name>
  <url-pattern>/execute</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>one</servlet-name>
  <servlet-class>MvcPackage.UpdateServlet</servlet-class>
  </servlet>
  
   <servlet-mapping>
  <servlet-name>one</servlet-name>
  <url-pattern>/doSum</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>new</servlet-name>
    <servlet-class>MvcPackage.DeleteServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>new</servlet-name>
    <url-pattern>/delete</url-pattern>
</servlet-mapping> 

  
  
</web-app>